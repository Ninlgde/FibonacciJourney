    "# 第三种解法--迭代法"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "fourth-enclosure",
   "metadata": {},
   "outputs": [],
   "source": [
    "# To iterate is human,to recurse divine.\n",
    "def fib_iterate(n):\n",
    "    a, b = 1.0, 0.0\n",
    "    for _ in range(n-1):\n",
    "        a, b = a + b, a # => t=a;a=a+b;b=t\n",
    "    return a"
   ]
  },
